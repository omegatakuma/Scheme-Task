#!/usr/local/bin/gosh
(use srfi-1)
(use srfi-19)
(use gauche.process)
(use file.util)
(use gauche.parseopt)
(use taskf.cmd)
(define (main args)
  (let-args (cdr args)
			((w "w|write" => w)
			 (r "r|read" => r)
			 (d "d|del" => del))
			(clear)
			(print "YES,TaskList!!!!!!!!")
			(print "-------------------")
			(ToDo)))
